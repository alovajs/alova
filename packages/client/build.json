{
  "core": {
    "input": "src/index.ts",
    "output": "../alova/dist/clienthook/index.{suffix}.{ext}",
    "packageName": "AlovaClientHook",
    "external": {
      "alova": "alova",
      "@alova/shared": null
    }
  },
  "vue": {
    "packageName": "vueHook",
    "input": "src/statesHook/vue.ts",
    "external": {
      "vue": "Vue",
      "@alova/shared": null
    },
    "output": "../alova/dist/stateshook/vue.{suffix}.{ext}"
  },
  "vue-demi": {
    "packageName": "vueHook",
    "input": "src/statesHook/vue-demi.ts",
    "external": {
      "vue-demi": "Vue",
      "@alova/shared": null
    },
    "output": "../alova/dist/stateshook/vue-demi.{suffix}.{ext}"
  },
  "react": {
    "packageName": "reactHook",
    "input": "src/statesHook/react.ts",
    "external": {
      "react": "React",
      "@alova/shared": null
    },
    "output": "../alova/dist/stateshook/react.{suffix}.{ext}"
  },
  "svelte": {
    "packageName": "svelteHook",
    "input": "src/statesHook/svelte.ts",
    "external": {
      "svelte": null,
      "svelte/store": null,
      "@alova/shared": null
    },
    "formats": ["cjs", "esm"],
    "output": "../alova/dist/stateshook/svelte.{suffix}.{ext}"
  },
  "solid": {
    "packageName": "solidHook",
    "input": "src/statesHook/solid.ts",
    "external": {
      "solid-js": "Solid",
      "@alova/shared/function": null,
      "@alova/shared/vars": null,
      "@alova/shared/assert": null
    },
    "output": "../alova/dist/stateshook/solid.{suffix}.{ext}"
  }
}
