{
  "name": "@alova/client",
  "version": "2.0.0",
  "description": "client request strategies for alova.js",
  "homepage": "https://alova.js.org",
  "private": true,
  "type": "module",
  "keywords": [
    "hooks",
    "vuejs",
    "typescript",
    "vue",
    "request",
    "vue3",
    "superagent",
    "alova",
    "react",
    "svelte",
    "axios"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "test": "jest && npm run test:node",
    "test:node": "jest --config jest.config.node.ts",
    "lint": "eslint --ext .ts,.js src/**",
    "lint:fix": "eslint --ext .ts,.js src/** --fix",
    "build": "npm run clean && alova-scripts build && ncp ./typings/clienthook ../alova/typings/clienthook && ncp ./typings/stateshook ../alova/typings/stateshook",
    "commit": "git-cz && git push"
  },
  "author": "Scott hu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/alovajs/alova.git"
  },
  "bugs": {
    "url": "https://github.com/alovajs/alova/issues"
  },
  "peerDependencies": {
    "alova": "^3.0.0-beta.3"
  },
  "dependencies": {
    "@alova/shared": "workspace:*"
  },
  "devDependencies": {
    "@alova/mock": "workspace:*",
    "@alova/scripts": "workspace:*",
    "@types/eventsource": "^1.1.15",
    "alova": "workspace:*",
    "eventsource": "^2.0.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
